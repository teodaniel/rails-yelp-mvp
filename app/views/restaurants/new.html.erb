<h1>New Restaurant</h1>
<p>Find me in app/views/restaurant/new.html.erb</p>

<div class="container">
  <%= simple_form_for @restaurant do |f| %>
  <%= f.input :name %>
  <%= f.input :address %>
  <%= f.input :phone_number %>
  <%= f.input :category %>
  <%= f.button :submit, class: 'btn btn-primary btn-block' %>
  <% end %>
</div>

